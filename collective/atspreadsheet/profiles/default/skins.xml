<?xml version="1.0"?>
<object name="portal_skins" meta_type="Plone Skins Tool" allow_any="False"
   cookie_persistence="False"
   request_varname="plone_skin"
   based-on="Plone Default">

  <object name="collective_atspreadsheet"
          meta_type="Filesystem Directory View"
          directory="collective.atspreadsheet:skins/collective_atspreadsheet" />


  <skin-path name="*">
    <layer name="collective_atspreadsheet" insert-after="custom" />
  </skin-path>

</object>

